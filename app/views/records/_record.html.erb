<tr id="<%= dom_id record %>">
  <th scope="col">
      <%= button_to "Stop", record, method: :stop(record), class:"btn btn-primary"%>
  </th>
  <th scope="col"><%=record.id %></th>
  <th scope="col">
      <%= record.created_at.strftime('%-I:%M %p')  %>
  </th>
  <th scope="col">
      <%= record.updated_at.strftime('%-I:%M %p') %>
  </th>
  <th scope="col">
      <%= button_to "x", record, method: :delete, class:"btn-danger btn" %>
  </th>
</tr>
